    "n = int(input(\"Enter a number:\"))\n",
    "s = 0\n",
    "while n!= 0:\n",
    "    rem = n%10\n",
    "    s += rem\n",
    "\n",
    "    n = n//10\n",
    "\n",
    "print(s)"
